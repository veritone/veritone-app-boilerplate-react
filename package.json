{
  "name": "veritone-app-boilerplate-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mitch Robb <mrobb@veritone.com>",
  "private": false,
  "now": {
    "alias": "veritone-app-boilerplate-react"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "deploy": "now && now alias",
    "test:watch": "jest --watch",
    "now-start": "serve -s",
    "format": "prettier --write \"./**/*.{js,css,scss,md,json}\"",
    "lint": "yarn run lint:js && yarn run lint:styles",
    "lint:js": "eslint ./ --max-warnings 0",
    "lint:styles": "stylelint \"./**/*.{css,scss}\""
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@neutrinojs/jest": "^9.0.0-rc.4",
    "@neutrinojs/react": "^9.0.0-rc.4",
    "@neutrinojs/style-loader": "^9.0.0-rc.4",
    "babel-eslint": "^8.2.3",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-universal-import": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^1.0.0-rc.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^7.2.2",
    "neutrino": "^9.0.0-rc.4",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.0",
    "now": "^11.2.4",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "sass-loader": "^7.0.1",
    "stylelint": "^8.3.0",
    "stylelint-config-standard": "^17.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "veritone-dev-env": "^2.0.0",
    "webpack": "4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{js,css,scss,md,json}": [
      "prettier --write",
      "git add"
    ],
    "./**/*.js": [
      "eslint --max-warnings 0",
      "git add"
    ],
    "./**/*.{scss,css}": [
      "stylelint",
      "git add"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "babel-merge": "^1.1.1",
    "date-fns": "^1.29.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "react-redux": "^5.0.7",
    "react-universal-component": "4.0.0",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-api-middleware-fixed": "^2.0.0",
    "redux-first-router": "^0.0.9-rudy",
    "redux-first-router-link": "^1.4.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "serve": "^9.1.0",
    "veritone-react-common": "^9.0.0",
    "veritone-redux-common": "^3.7.0",
    "veritone-widgets": "^8.0.4"
  }
}
